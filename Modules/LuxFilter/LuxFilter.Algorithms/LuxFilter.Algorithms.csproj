	<RuntimeIdentifiers>win-x86;win-x64;win-arm64</RuntimeIdentifiers>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Algorithms\PerceptualMetrics\brisque_model_live.yml" />
    <None Remove="Algorithms\PerceptualMetrics\brisque_range_live.yml" />
  </ItemGroup>

  <ItemGroup>
